.Body-create
  display inline-block
  -webkit-box-sizing border-box
  box-sizing border-box
  background #eee
  position absolute
  height 100%
.userCreate
  margin-left 20px
  &-label
    width 100%
  &-button
    display block
    margin 10px 0px 0 55px
    border-radius 20px
    width 8em
  &-input
    width 16em
    border-radius 20px
    border-style none
    padding 5px 10px 5px 10px