.body
  overflow-x hidden
  overflow-y hidden
.Menu-bar
  width 240px
  height 100%
  background #37485b
  padding-top 50px
  position absolute
  border-right 1px solid #999
  z-index 1
.log
  float right
  padding 10px 10px
  font-size 20px
  color #fff
  position relative
.log:hover
  text-decoration none
  color #fff
.log:focus
  text-decoration none
  color #fff
.log2
  float left
  padding-left 10px
  font-family 'Microsoft Sans Serif'
  color #fff
  font-size 35px
  position relative
.Menu-logo
  background #303a46
  text-align center
  font-size 10px
  padding-top 10px
  padding-bottom 12px
.Menu-items
  margin 0
  padding 0
  overflow-y scroll
  overflow-x hidden
  height 503px
.Menu-list
  list-style none
.Menu-links
  font-size 15px
  display block
  padding 10px
  text-decoration none
  color lightgray
  -webkit-transition 0.2s
  -o-transition 0.2s
  transition 0.2s
.Menu-links:hover
  text-decoration none
  color #fff
  background #303a46
  padding-left 30px
.Menu-links:focus
  text-decoration none
  color #fff
  padding-left 30px
  background #303a46
.Menu-listin
  list-style none
.Menu-linksin
  display block
  padding 10px
  color #fff
  text-decoration none
  -webkit-transition 0.2s
  -o-transition 0.2s
  transition 0.2s
.Menu-linksin:hover
  text-decoration none
  color #fff
  background #303a46
  cursor pointer
.Menu-linksin:focus
  text-decoration none
  color #fff
  background #303a46
  cursor pointer
.Menu-itemsin
  margin 0
  padding 0
  display none
.contenido
  -webkit-box-sizing border-box
  box-sizing border-box
  background #eee
  overflow-x scroll
  overflow-y scroll
  margin-top 45px
  margin-left 240px
  width 82.5%
  height 593.4px
  margin-right 0px
  margin-bottom 0px
  padding 0px 0px 0px 0px
  &-nav
    width 108%
    display flex
    align-items center
    padding-bottom 5px
    border-bottom 1px solid #999
    border-top 1px solid #999
  &-label
    display inline-block
    margin-top 15px
    margin-left 20px
    margin-right 10px
    font-weight bold
  &-number
    display inline-block
    margin-top 11px
    padding 5px 10px
    width 200px
  &-search
    display inline-block
    margin-top 10px
    padding 4px 11px
  &-pages
    display inline-block
    position relative
    left 260px
    top 4px
  &-nav-actions
    margin-top 10px
    left 10px
    padding 5px 10px
    position relative
.pages-icon_bolt
  display inline-block
  position relative
  right 10px
  font-size 25px
  top 4px
.pages-icon
  display inline-block
  font-size 18px
  position relative
  top 1.5px
.pages-icon2
  display inline-block
  font-size 25px
  position relative
  top 4px
.pages-input
  display inline-block
.nav-actions
    color #000
    text-decoration none
    padding 8px 8px
    border-radius 5px
    border 0.5px solid #eee
    font-weight bold
    -webkit-transition all 0.2s
    -o-transition all 0.2s
    transition all 0.2s
.nav-actions:hover
  color #000
  text-decoration none
  background #149BE6
  border 0.5px solid #278efc
  color #fff
.nav-actions:focus
  color #fff
  text-decoration none
  background #149BE6
  border 0.5px solid #278efc
.Menu-list:hover .Menu-itemsin
  display block
  position absolute
.barra
  display inline-block
  position relative
  cursor pointer
  padding 5px 10px
  left 10px
  top 5px
  font-size 20px
  border 1px solid #999
  border-radius 5px
  -webkit-transition all 0.2s
  -o-transition all 0.2s
  transition all 0.2s
.barra:hover
  background #999
  border 1px solid #eee
  color #fff
.abrir
  transform translateX(300px)
.Tabla
  -webkit-box-sizing border-box
  box-sizing border-box
  max-width 100%
  max-height 100%
  border-bottom 1px solid #999
  &-hfilas
    width 300px
    padding 5px
    text-align center
    border-bottom 1px solid #999
    background #fff
  &-hfilas_special
    padding-top 8px
    padding-bottom 5px
    padding-right 20px
    padding-left 20px
    border-bottom 1px solid #999
    background #fff
  &-filas
    width 300px
    padding-top 14px
    padding-bottom 10px
    text-align center
    background #fff
    border-bottom 1px solid #999
  &-filas_special
    padding-top 14px
    padding-left 20px
    padding-right 20px
    padding-bottom 10px
    background #fff
    border-bottom 1px solid #999
  &-icon
    cursor pointer
  &-icon2
    cursor pointer
    color #149BE6
    font-size 18px
  &-icon_body
    cursor pointer
    color #149BE6
    font-size 20px
.Up-tabla
  width 100%
  padding-left 15px
  background #fff
  padding-top 2px
  padding-bottom 2px
.icon-Up,.icon-Up-2
  display inline-block
  color #149BE6
.icon-Up
  font-size 30px
.icon-Up-2
  padding-left 20px
  font-weight bold
.search-input
  width 200px
  border-radius 15px
  border 1px solid lightgray
  background #303a46
  padding 5px 0 5px 25px
  left 7px
  top 2px
  position relative
  font-size 12px
  color #fff
  outline-style none
.search-input::-webkit-input-placeholder
  color lightgray
.search-input:focus
  outline-style none
.input-icon
  position relative
  right 188px
  color lightgray
  font-size 17px
  top 4px
.After-nav a
  display inline-block
  padding 10px 33.3px
  background #303a46
  text-decoration none
  color #999
  -webkit-transition all 0.2s
  -o-transition all 0.2s
  transition all 0.2s
.After-nav a:hover
  background #4b545f
  color lightgray
.After-nav a:focus
  background #4b545f
  color lightgray
.wrapper
  width 100%
  max-width 2000px
  overflow hidden
  margin auto
